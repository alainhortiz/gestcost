<div id="modalEditPrecioLubricante" data-backdrop="static" data-keyboard=false class="modal fade" tabindex="-1"
     aria-hidden="true">
    <div class="modal-dialog" style="width: 50vw">
        <div class="modal-content portlet light estilosForm">
            <div class="modal-header">
                <div class="caption font-green">
                    <i class="icon-settings font-green-sunglo"></i>
                    <span class="bold uppercase" style="font-size: 17px"> MODIFICAR PRECIO DEl ACEITE</span>
                </div>
            </div>
            <div class="modal-body">
                <div class="scroller" style="height: 180px" data-always-visible="1" data-rail-visible1="1">
                    <div class="row">
                        <div class="portlet-body form">
                            <form class="form-horizontal" role="form">
                                <div class="form-body">
                                    <div class="form-group">
                                        <label class="col-lg-3 col-md-3 col-sm-3 control-label">Year</label>
                                        <div class="col-lg-8 col-md-8 col-sm-8">
                                            <div class="input-group input-icon right">
												<span class="input-group-addon">
												<i class="fa fa-tag"></i>
												</span>
                                                <i style="color: #dc141b"
                                                   class="fa fa-exclamation tooltips hidden editYear"
                                                   data-placement="bottom"
                                                   data-original-title="Este campo es obligatorio"></i>
                                                <input type="text" class="form-control"
                                                       id="editYear" name="editYear" readonly disabled>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-lg-3 col-md-3 col-sm-3 control-label">Mes</label>
                                        <div class="col-lg-8 col-md-8 col-sm-8">
                                            <div class="input-group input-icon right">
												<span class="input-group-addon">
												<i class="fa fa-tag"></i>
												</span>
                                                <i style="color: #dc141b"
                                                   class="fa fa-exclamation tooltips hidden editMes"
                                                   data-placement="bottom"
                                                   data-original-title="Este campo es obligatorio"></i>
                                                <input type="text" class="form-control"
                                                       id="editMes" name="editMes" readonly disabled>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-lg-3 col-md-3 col-sm-3 control-label">Precio</label>
                                        <div class="col-lg-8 col-md-8 col-sm-8">
                                            <div class="input-group input-icon right">
												<span class="input-group-addon">
												<i class="fa fa-tag"></i>
												</span>
                                                <i style="color: #dc141b"
                                                   class="fa fa-exclamation tooltips hidden editPrecio"
                                                   data-placement="bottom"
                                                   data-original-title="Este campo es obligatorio"></i>
                                                <input type="text" class="form-control" onkeypress="return filterFloat(event,this);"
                                                       id="editPrecio" name="editPrecio">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="modal-footer">
            <button type="button" data-dismiss="modal" class="btn default cerrar">Cerrar</button>
            <button type="button" class="btn btn-secondary" id="editGuardar">Guardar</button>
        </div>
    </div>
</div>
